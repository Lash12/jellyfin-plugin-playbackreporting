  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Active Playbacks by Mode",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (play_mode) (jellyfin_active_playbacks_by_mode)",
          "legendFormat": "{{play_mode}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Playback Starts Rate (5m) by Mode",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum by (play_mode) (rate(jellyfin_playback_mode_starts_total[5m]))",
          "legendFormat": "{{play_mode}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Transcode Share (5m)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "100 * sum(rate(jellyfin_playback_mode_starts_total{play_mode=\"transcode\"}[5m])) / sum(rate(jellyfin_playback_mode_starts_total[5m]))",
          "legendFormat": "transcode_share_pct",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Playback Duration Quantiles (10m)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.5, sum(rate(jellyfin_playback_duration_seconds_bucket[10m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.9, sum(rate(jellyfin_playback_duration_seconds_bucket[10m])) by (le))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum(rate(jellyfin_playback_duration_seconds_bucket[10m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    }
  ],
  "refresh": "30s",
